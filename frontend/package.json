{
  "name": "@inecas/persai",
  "version": "0.1.0",
  "scripts": {
    "dev": "rsbuild dev",
    "build": "npm run build-mf && concurrently \"npm:build:*\"",
    "build-mf": "rsbuild build",
    "build:cjs": "swc ./src -d dist/lib/cjs --strip-leading-paths --config-file .cjs.swcrc",
    "build:esm": "swc ./src -d dist/lib --strip-leading-paths --config-file .swcrc",
    "build:types": "tsc --project tsconfig.build.json",
    "test": "cross-env LC_ALL=C TZ=UTC jest",
    "test:watch": "cross-env LC_ALL=C TZ=UTC jest --watch",
    "test:coverage": "cross-env LC_ALL=C TZ=UTC jest --coverage",
    "type-check": "tsc --noEmit"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@module-federation/rsbuild-plugin": "^0.11.2",
    "@rsbuild/core": "^1.2.19",
    "@rsbuild/plugin-react": "^1.1.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.7.10",
    "@swc/jest": "^0.2.37",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.14",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.26.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@hookform/resolvers": "^3.2.0",
    "@perses-dev/components": "^0.51.0-beta.1",
    "@perses-dev/core": "^0.51.0-beta.1",
    "@perses-dev/dashboards": "^0.51.0-beta.1",
    "@perses-dev/explore": "^0.51.0-beta.1",
    "@perses-dev/plugin-system": "^0.51.0-beta.1",
    "@perses-dev/prometheus-plugin": "^0.51.0",
    "@tanstack/react-query": "^4.39.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "echarts": "5.5.0",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0",
    "react-hook-form": "^7.52.2",
    "use-resize-observer": "^9.0.0"
  },
  "files": ["lib/**/*", "__mf/**/*", "mf-manifest.json", "mf-stats.json"],
  "perses": {
    "moduleName": "Persai",
    "moduleOrg": "inecas",
    "schemasPath": "schemas",
    "plugins": [
      {
        "kind": "Explore",
        "spec": {
          "display": {
            "name": "PersAI Explorer"
          },
          "name": "PersaiExplorer"
        }
      }
    ]
  },
  "dependencies": {
    "llama-stack-client": "^0.2.14",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1"
  }
}
